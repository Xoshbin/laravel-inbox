{
  "name": "xoshbin/laravel-inbox",
  "description": "Receive and send emails from your project with Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Khoshbin Ali",
      "email": "xoshbin@gmail.com"
    }
  ],
  "require": {
    "beyondcode/laravel-mailbox": "^1.3"
    },
  "autoload": {
    "psr-4": {
      "Xoshbin\\Inbox\\": "src"
    }
  },
  "autoload-dev": {
      "psr-4": {
          "Xoshbin\\Inbox\\Tests\\": "tests"
      }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-f": "vendor/bin/phpunit --filter"
  },
  "extra": {
      "laravel": {
          "providers": [
              "Xoshbin\\Inbox\\InboxServiceProvider"
          ],
          "aliases": {
            "Inbox": "Xoshbin\\Inbox\\Facades\\Inbox"
            }
      }
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^9.0"
  }
}
